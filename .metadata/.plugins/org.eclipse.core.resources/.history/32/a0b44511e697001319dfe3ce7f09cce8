package com.example.bump.actions;

public class Color {
	private int rouge;
	private int vert;
	private int bleu;
	
	public Color (int rouge, int vert, int bleu) {
		this.rouge = rouge;
		this.vert = vert;
		this.bleu = bleu;
	}
	
	@Override
	
	public String toString() {
		return rouge+" "+vert+" "+bleu+"";
	}
}
